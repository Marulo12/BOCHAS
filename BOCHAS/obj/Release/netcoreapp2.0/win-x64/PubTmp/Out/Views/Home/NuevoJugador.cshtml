
@{

    Layout = null;
}
<!Doctype html>
<html>
<head>
    
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    
    <link href="~/Menu/assets/css/main.css" rel="stylesheet" />
    <link href="~/lib/Alertify/alertify.css" rel="stylesheet" />
    <link href="~/lib/Alertify/themes/bootstrap.css" rel="stylesheet" />
    <link rel="icon" type="image/png" href="~/images/LogoBochas.png" />
    <title>BOCHAS PADEL</title>
</head>
<body id="altaJugador" style=" background: linear-gradient(135deg, #cc0000 0%,#0a1896 0%,#cccccc 46%,#cccccc 58%,#cc0000 100%);height:100%;padding-top:2%;">
    <div class="container-fluid" ng-app="">
        <div class="panel panel-default">
            <div class="panel-heading" style="padding:0px;background-color:white;margin-bottom:0px;">
                <div class="row">
                    <div class="col-sm-4"><img src="~/images/extras/racchette-test-img.png" width="160" /></div>
                    <div class="col-sm-3">
                        <h2 style="text-align:center;font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif">Nueva Cuenta</h2>
                        <h4 style="text-align:center;">Ingrese sus datos personales</h4>
                    </div>
                    <div class="col-md-4"></div>
                </div>
            </div>
            <div class="panel-body">
                <form class="form-group-sm">
                    <div class="row">

                        <div class="col-sm-3">

                            <div class="form-group">
                                <label>Nombre</label>
                                <input id="Nombre" ng-model="Nombre" class="form-control" />

                                <br />
                                <label>Localidad</label>
                                <select id="Localidad" class="form-control">
                                    <option>Cargando...</option>
                                </select>


                            </div>
                        </div>

                        <div class="col-sm-3">

                            <div class="form-group">
                                <label>Apellido</label>
                                <input id="Apellido" ng-model="Apellido" class="form-control" />

                                <br />
                                <label>Barrio</label>
                                <select id="Barrio" class="form-control"><option>Cargando...</option></select>

                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label>Tipo Documento</label>
                                <select id="TipoDoc" class="form-control"><option>Cargando...</option></select>
                                <br />
                                <label>Calle</label>
                                <input id="Calle" class="form-control" />


                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="row">
                                <div class="form-group">
                                    <label>N° Documento</label>

                                    <input id="Numero" class="form-control" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group">
                                    <div class="col-sm-4">
                                        <label>N°</label>

                                        <input id="NCalle" class="form-control" />

                                    </div>
                                    <div class="col-sm-4">
                                        <label>Piso</label>
                                        <input class="form-control" id="Piso" />

                                    </div>
                                    <div class="col-sm-4">
                                        <label class="control-label">Dpto</label>
                                        <input class="form-control" id="Dpto" />

                                    </div>

                                </div>
                            </div>


                        </div>




                    </div>
                    <div class="row">
                        <div class="col-sm-3">
                            <div class="form-group">

                                <label>Mail</label>
                                <input id="Mail" class="form-control" />
                                <br />
                                <div id="TipoJugador">


                                </div>

                            </div>
                        </div>

                        <div class="col-sm-3">

                            <div class="form-group">

                                <label>Teléfono</label>
                                <input id="Telefono" class="form-control" />


                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label class="control-label">Usuario para el sistema</label>
                                <input id="Usuario" value="{{Nombre.charAt(0) + Apellido}}" placeholder="Usuario" class="form-control" />


                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label class="control-label">Contraseña</label>
                                <input type="password" id="Contra" class="form-control" />

                                <br />
                                <div class="row">
                                    <div class="col-sm-6"><input type="button" id="Registrar" value="Crear" class="btn btn-success form-control" style="margin-top:6px;color:white;" /></div>
                                    <div class="col-sm-6"><a asp-action="Index" asp-controller="Usuarios"><input type="button" value="Volver" class="btn btn-default form-control" style="margin-top:6px;" /></a></div>
                                </div>


                            </div>
                        </div>
                    </div>

                </form>
            </div>
        </div>

    </div>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
  

    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.9/angular.min.js"></script>

    <script src="~/lib/Alertify/alertify.js"></script>
    <script src="~/js/JugadorExterno.js"></script>
</body>
</html>